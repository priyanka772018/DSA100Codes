#include<iostream>
using namespace std;
int main() {
    int n;
    cin>>n;
    if(n%2==0){
        cout<<"number is even "<<endl;
    }
    else{
        cout<<"number is odd"<<endl;
    }
    return 0;
    
}


/*Trenary Operator syntax
(Condition)?(if True:Action):(if False:Action);
n%2==0?cout<<"odd"<<"Even";
*/